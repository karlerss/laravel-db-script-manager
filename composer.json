{
    "name": "karlerss/laravel-db-script-manager",
    "description": "Manage database views, stored procedures etc with Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Karl-Sander Erss",
            "email": "karl.erss@gmail.com"
        }
    ],
    "homepage": "https://github.com/karlerss/laravel-db-script-manager",
    "keywords": ["Laravel", "Database migrations", "Database views"],
    "require": {
        "illuminate/support": "~7"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~5.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Karlerss\\LaravelDbScriptManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karlerss\\LaravelDbScriptManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karlerss\\LaravelDbScriptManager\\LaravelDbScriptManagerServiceProvider"
            ],
            "aliases": {
                "LaravelDbScriptManager": "Karlerss\\LaravelDbScriptManager\\Facades\\LaravelDbScriptManager"
            }
        }
    }
}
